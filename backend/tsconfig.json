{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS", // Use CommonJS instead of ESNext
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "useUnknownInCatchVariables": false,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  